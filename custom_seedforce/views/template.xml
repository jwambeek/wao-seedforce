<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="contact_form_data">
    <!--<p>Test Template</p>-->

    <div class="oe_structure clearfix mt-3" id="oe_structure_website_sale_payment_1">
      <section class="s_website_form pt16 pb16 o_colored_level" data-vcss="001" data-snippet="s_website_form" data-name="Form">
        <div class="container">
          <form action="/website_form/" method="post" enctype="multipart/form-data" class="o_mark_required" data-mark="*" data-success-mode="redirect" data-success-page="/contactus-thank-you" data-model_name="mail.mail">
            <div class="s_website_form_rows row s_col_no_bgcolor">
              <div class="form-group s_website_form_field col-12    s_website_form_dnone" data-name="Field">
                <div class="row s_col_no_resize s_col_no_bgcolor">
                  <label class="col-form-label col-sm-auto s_website_form_label" style="width: 200px">
                    <span class="s_website_form_label_content"/>
                    <span title="Separate email addresses with a comma.">
                      <i class="fa fa-info-circle"/>
                    </span>
                  </label>
                  <div class="col-sm">
                    <input type="hidden" class="form-control s_website_form_input" name="email_to" value="info@yourcompany.example.com"/>
                  </div>
                </div>
              </div>
              <div class="form-group s_website_form_field col-12 s_website_form_custom s_website_form_required  " data-type="char" data-name="Field">
                <div class="row s_col_no_resize s_col_no_bgcolor">
                  <label class="col-form-label col-sm-auto s_website_form_label " style="width: 200px" for="h1vk9merhjb">
                    <span class="s_website_form_label_content">Retailer Name</span>
                    <span class="s_website_form_mark"> *</span>
                  </label>
                  <div class="col-sm">
                    <input type="text" class="form-control s_website_form_input" name="Retailer Name" required="1" id="h1vk9merhjb" placeholder="" />
                  </div>
                </div>
              </div>
              <div class="form-group s_website_form_field col-12 s_website_form_custom col-lg-7" data-type="char" data-name="Field">
                <div class="row s_col_no_resize s_col_no_bgcolor">
                  <label class="col-form-label col-sm-auto s_website_form_label " style="width: 200px" for="gatc9msf9g9">
                    <span class="s_website_form_label_content">NGR Number</span>
                  </label>
                  <div class="col-sm">
                    <input type="text" class="form-control s_website_form_input" name="NGR Number" placeholder="" id="gatc9msf9g9"/>
                  </div>
                </div>
              </div>
              <div class="form-group s_website_form_field col-12 s_website_form_custom col-lg-12" data-type="char" data-name="Field">
                <div class="row s_col_no_resize s_col_no_bgcolor">
                  <label class="col-form-label col-sm-auto s_website_form_label " style="width: 200px" for="fyzqi80e44g">
                    <span class="s_website_form_label_content">Trading Address</span>
                  </label>
                  <div class="col-sm">
                    <input type="text" class="form-control s_website_form_input" name="Trading Address" placeholder="" id="fyzqi80e44g"/>
                  </div>
                </div>
              </div>
              <div class="form-group s_website_form_field col-12 s_website_form_custom col-lg-8" data-type="char" data-name="Field">
                <div class="row s_col_no_resize s_col_no_bgcolor">
                  <label class="col-form-label col-sm-auto s_website_form_label " style="width: 200px" for="bnljoii9zjs">
                    <span class="s_website_form_label_content">Town/City</span>
                  </label>
                  <div class="col-sm">
                    <input type="text" class="form-control s_website_form_input" name="Town/City" id="bnljoii9zjs"/>
                  </div>
                </div>
              </div>
              <div class="form-group s_website_form_field col-12 col-lg-8 s_website_form_custom   " data-type="char" data-name="Field">
                <div class="row s_col_no_resize s_col_no_bgcolor">
                  <label class="col-form-label col-sm-auto s_website_form_label " style="width: 200px" for="efvhjzosqwm">
                    <span class="s_website_form_label_content">Post Code</span>
                  </label>
                  <div class="col-sm">
                    <input type="text" class="form-control s_website_form_input" name="Post Code" placeholder="" id="efvhjzosqwm"/>
                  </div>
                </div>
              </div>
              <div class="form-group s_website_form_field col-12 col-lg-9 s_website_form_custom s_website_form_required" data-type="many2one" data-name="Field">
                <div class="row s_col_no_resize s_col_no_bgcolor">
                  <label class="col-form-label col-sm-auto s_website_form_label " style="width: 200px" for="uhknodhfrwj">
                    <span class="s_website_form_label_content">State</span>
                    <span class="s_website_form_mark"> *</span>
                  </label>
                  <div class="col-sm">
                    <select class="form-control s_website_form_input" name="State" id="uhknodhfrwj" required="" style="">
                      <option value="WA">WA</option>
                      <option value="VIC">VIC</option>
                      <option value="QLD">QLD</option>
                      <option value="NSW">NSW</option>
                      <option value="TAS">TAS</option>
                      <option value="SA">SA</option>
                      <option value="ACT">ACT</option>
                    </select>
                  </div>
                </div>
              </div>
              <div class="form-group s_website_form_field col-12 col-lg-9 s_website_form_custom   " data-type="tel" data-name="Field">
                <div class="row s_col_no_resize s_col_no_bgcolor">
                  <label class="col-form-label col-sm-auto s_website_form_label " style="width: 200px" for="lknjwkfgdf">
                    <span class="s_website_form_label_content">Mobile</span>
                  </label>
                  <div class="col-sm">
                    <input type="tel" class="form-control s_website_form_input" name="Mobile" placeholder="" id="lknjwkfgdf"/>
                  </div>
                </div>
              </div>
              <div class="form-group s_website_form_field col-12  s_website_form_model_required  " data-type="char" data-name="Field">
                <div class="row s_col_no_resize s_col_no_bgcolor">
                  <label class="col-form-label col-sm-auto s_website_form_label " style="width: 200px" for="qu2bagath7">
                    <span class="s_website_form_label_content">Email</span>
                    <span class="s_website_form_mark">         *</span>
                  </label>
                  <div class="col-sm">
                    <input type="text" class="form-control s_website_form_input" name="email_from" required="true" placeholder="" id="qu2bagath7"/>
                  </div>
                </div>
              </div>
             <!-- <div class="form-group s_website_form_field col-12  s_website_form_model_required  " data-type="char" data-name="Field">
                <div class="row s_col_no_resize s_col_no_bgcolor">
                  <label class="col-form-label col-sm-auto s_website_form_label " style="width: 200px" for="if5z3dum0wg">
                    <span class="s_website_form_label_content">Subject</span>
                    <span class="s_website_form_mark"> *</span>
                  </label>
                 <div class="col-sm">
                    <input type="text" class="form-control s_website_form_input" name="subject" required="true" id="if5z3dum0wg"/>
                  </div> 
                </div>
              </div> -->
              <div class="form-group col-12 s_website_form_submit" data-name="Submit Button">
                <div style="width: 200px;" class="s_website_form_label"/>
                <a href="#" role="button" class="btn btn-primary btn-lg s_website_form_send o_default_snippet_text">Submit</a>
                <span id="s_website_form_result"/>
              </div>
            </div>
          </form>
        </div>
      </section>
    </div>








    </template>
   
